{
	"name": "vite-react-ts-template",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "tsc -b && bunx --bun vite build",
		"format": "bunx --bun @biomejs/biome format ./src --write",
		"lint": "bunx --bun @biomejs/biome lint ./src --write",
		"preview": "bunx --bun vite preview"
	},
	"dependencies": {
		"@tailwindcss/postcss": "^4.1.7",
		"@tailwindcss/vite": "^4.1.7",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@eslint/js": "^9.15.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^v10.4.21",
		"globals": "^15.12.0",
		"tailwindcss": "^v4.1.7",
		"typescript": "~5.6.2",
		"vite": "^6.0.1"
	}
}
